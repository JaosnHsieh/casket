{
	"name": "casket",
	"description": "casket is an easy-to-use web file storage.",
	"version": "0.1.0",
	"keywords": [
		"casket",
		"file",
		"storage",
		"web",
		"http",
		"upload",
		"files",
		"coffeescript",
		"javascript"
	],
	"author": {
		"name": "Jannis R",
		"email": "mail@jannisr.de",
		"url": "http://jannisr.de"
	},
	"homepage": "https://github.com/derhuerst/casket",
	"repository": {
		"type": "git",
		"url": "git://github.com/derhuerst/casket.git"
	},
	"bugs": "https://github.com/derhuerst/casket/issues",
	"license": "MIT",
	"dependencies": {
		"coffee-script":	"^1.9.3",
		"minimist":			"^1.1.1",
		"winston":			"^1.0.1",
		"http-error":		"^0.0.5",
		"accepts":			"^1.2.11",
		"swig":				"^1.4.2",
		"send":				"^0.12.0",
		"forever":			"^0.14.2"
	},
	"main": "src/Casket.coffee",
	"bin": {
		"casket": "./bin/casket"
	},
	"scripts": {
		"casket":	"./bin/casket",
		"start":	"./bin/forever start -c coffee ./bin/casket",
		"watch":	"./bin/forever start -w --watchDirectory ./server --watchIgnore ./node_modules/ -c coffee ./bin/casket",
		"stop":		"./bin/forever stop ./bin/casket",
		"restart":	"./bin/forever restart ./bin/casket",
		"logs":		"./bin/forever logs ./bin/casket"
	},
	"directories": {
		"lib": "./server",
		"bin": "./bin"
	}
}
